{
  "name": "yalento",
  "version": "0.0.90",
  "description": "An awesome integration of Google Firebase for Angular and Node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "npm run build && nyc --reporter=lcov --reporter=text-summary node_modules/.bin/mocha --opts ./mocha.opts src",
    "test:ng8": "npm run build && cp -R lib test/ng8/node_modules/yalento/lib && cd test/ng8 && npm run test-headless"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/"
  },
  "keywords": [
    "firebase",
    "angular",
    "node.js",
    "alasql",
    "sequelizer",
    "typeorm",
    "state"
  ],
  "author": "C-J",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miegli/yalento/issues"
  },
  "homepage": "https://github.com/miegli/yalento/blob/master/README.md",
  "dependencies": {
    "@firebase/firestore-types": "^1.10.3",
    "@types/bluebird": "^3.5.32",
    "@types/validator": "^13.0.0",
    "alasql": "^0.6.2",
    "class-transformer": "^0.3.1",
    "core-js": "^3.6.5",
    "es6-shim": "^0.35.5",
    "firebase": "^7.14.6",
    "guid-typescript": "^1.0.9",
    "js-sql-parser": "^1.3.0",
    "ngeohash": "^0.6.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@firebase/testing": "^0.19.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.11",
    "@types/core-js": "^2.5.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.10",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.0.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.1.0",
    "lodash.camelcase": "^4.3.0",
    "log-update": "^4.0.0",
    "mocha": "^7.2.0",
    "mochawesome": "^6.1.1",
    "nyc": "^15.1.0",
    "pegjs": "^0.10.0",
    "prettier": "^2.0.5",
    "prompt": "^1.0.0",
    "prompt-input": "^3.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.3"
  },
  "files": [
    "lib/**/*"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
